/**
 * @see https://www.electron.build/configuration/configuration
 */
{
  appId: "YourAppID",
  asar: true,
  directories: {
    buildResources: "dist",
    output: "release/${version}",
  },
  files: ["dist"],
  mac: {
    artifactName: "${productName}_${version}.${ext}",
    target: ["dmg"],
    icon: "./icons/Burning Orange/android-chrome-512x512.png",
  },
  win: {
    target: [
      {
        target: "nsis",
        arch: ["x64"],
      },
    ],
    icon: "./icons/Burning Orange2/apple-touch-icon.png",
    artifactName: "${productName}_${version}.${ext}",
  },
  nsis: {
    oneClick: false,
    perMachine: false,
    allowToChangeInstallationDirectory: true,
    deleteAppDataOnUninstall: false,
  },
}
